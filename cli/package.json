{
  "name": "leapp",
  "description": "Leapp's Command Line Interface",
  "version": "0.1.0",
  "author": "besharp <info@besharp.it>",
  "bin": {
    "leapp": "./bin/run"
  },
  "bugs": "https://github.com/Noovolari/leapp/issues",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@noovolari/leapp-core": "file:../core/dist",
    "@oclif/core": "1.3.2",
    "@oclif/plugin-help": "^5",
    "@types/inquirer": "^8.2.0",
    "@types/node": "^15.0.1",
    "@types/uuid": "^8.3.0",
    "assert": "2.0.0",
    "aws-sdk": "2.928.0",
    "aws-sdk-mock": "5.3.0",
    "chdir": "0.0.0",
    "class-transformer": "^0.4.0",
    "compare-versions": "^3.6.0",
    "copy-dir": "~1.3.0",
    "crypto-js": "~4.0.0",
    "es6-shim": "^0.35.6",
    "extract-zip": "~2.0.1",
    "fix-path": "~3.0.0",
    "follow-redirects": "1.14.0",
    "fs-extra": "~9.1.0",
    "fs-web": "1.0.1",
    "global-tunnel": "~1.2.0",
    "http-proxy-agent": "4.0.1",
    "https-proxy-agent": "5.0.0",
    "ini": "~2.0.0",
    "inquirer": "^8.2.0",
    "is-url": "^1.2.4",
    "jwt-decode": "~3.1.2",
    "keytar": "7.7.0",
    "ms": "^2.1.3",
    "node-log-rotate": "~0.1.5",
    "node-machine-id": "~1.1.12",
    "puppeteer": "~13.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "~3.0.2",
    "rxjs": "~6.6.7",
    "saml-encoder-decoder-js": "~1.0.1",
    "semver": "~7.3.5",
    "standard-version": "^9.3.0",
    "sudo-prompt": "~9.2.1",
    "tslib": "~2.2.0",
    "uuid": "~8.3.2",
    "wait-on": "^6.0.0",
    "zlib": "~1.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.9.4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "gushio": "~0.5.0",
    "jest": "^27.4.5",
    "oclif": "^2.4.0",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/noovolari/leapp",
  "keywords": [
    "oclif"
  ],
  "main": "dist/index.js",
  "oclif": {
    "bin": "leapp",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "session": {
        "description": "Sessions management"
      },
      "profile": {
        "description": "AWS named profiles management"
      },
      "idp-url": {
        "description": "Identity providers URL management"
      },
      "region": {
        "description": "AWS regions management"
      },
      "integration": {
        "description": "AWS integrations management"
      }
    },
    "macos": {
      "identifier": "cloud.leapp"
    }
  },
  "repository": "noovolari/leapp",
  "scripts": {
    "clean": "gushio gushio/target-clean.js",
    "build": "gushio gushio/target-build.js",
    "test": "jest",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "pack-mac": "oclif pack:macos",
    "pack-linux": "oclif pack:deb",
    "pack-win": "oclif pack:win",
    "postpack": "gushio gushio/target-post-pack.js",
    "version": "oclif readme && git add README.md",
    "list-session": "./bin/dev session list"
  },
  "types": "dist/index.d.ts"
}
